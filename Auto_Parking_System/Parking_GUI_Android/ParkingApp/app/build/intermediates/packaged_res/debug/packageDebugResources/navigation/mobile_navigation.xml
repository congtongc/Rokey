<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.rokey.parkingapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_parking"
        android:name="com.rokey.parkingapp.ui.parking.ParkingFragment"
        android:label="@string/title_parking"
        tools:layout="@layout/fragment_parking" />

    <fragment
        android:id="@+id/navigation_exit"
        android:name="com.rokey.parkingapp.ui.exit.ExitFragment"
        android:label="@string/title_exit"
        tools:layout="@layout/fragment_exit" />
</navigation> 